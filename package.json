{
	"private": true,
	"scripts": {
		"dev": "nuxi dev",
		"build": "nuxi build",
		"start": "node .output/server/index.mjs",
		"format": "prettier --write src/**/*.{json,ts,vue} *.{json,ts,vue}",
		"lint:ts": "eslint src/**/*.ts *.ts --no-error-on-unmatched-pattern",
		"lint:vue": "eslint src/**/*.vue *.vue --no-error-on-unmatched-pattern",
		"lint": "npm run lint:ts && npm run lint:vue",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"eslint": "^8.3.0",
		"eslint-plugin-nuxt": "^3.1.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.2",
		"nuxt3": "latest",
		"prettier": "^2.5.0"
	},
	"volta": {
		"node": "16.13.1",
		"npm": "8.2.0"
	}
}
